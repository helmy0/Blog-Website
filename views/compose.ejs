        <label for="title">Title</label>
        <input class="form-control" id = "title"type="text" name="titleName">
    </div>
  
    <div class="form-group">
        <label for="textArea">Post</label>
        <!-- <input class="form-control" id = "title"type="text" name="textPublished"> -->
